{
    "name": "get-relative-path",
    "displayName": "get-relative-path",
    "description": "get relative path",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.getRelativePath"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [{
            "command": "extension.getRelativePath",
            "title": "Get Relative Path"
        }],
        "menus": {
            "explorer/context": [{
                "command": "extension.getRelativePath",
                "when": "editorIsOpen"
            }]
        }
    },
    "scripts": {
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.39.0",
        "eslint": "^5.13.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "vscode-test": "^1.2.0"
    }
}